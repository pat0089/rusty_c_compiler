{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Rust project",
      "type": "lldb",
      "request": "launch",
      "program": "${workspaceFolder}/target/debug/rusty_c_compiler",
      "args": [
        "./a.c"
      ],
      "cwd": "${workspaceFolder}",
      "stopOnEntry": false,
      "sourceLanguages": [
        "rust"
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": ["./a.c"],
      "stopAtEntry": false,
      "externalConsole": false,
      "cwd": "/home/pat0089/Projects/rusty_c_compiler",
      "program": "/home/pat0089/Projects/rusty_c_compiler/build/Debug/rusty_c_compiler",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}